{
  "name": "challenge4-login-webapp",
  "version": "1.0.0",
  "description": "Web application that consumes the login API from challenge3",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "cypress run",
    "test:open": "cypress open",
    "test:login": "cypress run --spec 'cypress/e2e/login.cy.js'",
    "test:forgot": "cypress run --spec 'cypress/e2e/forgotPassword.cy.js'",
    "test:chrome": "cypress run --browser chrome",
    "test:headed": "cypress run --headed"
  },
  "keywords": [
    "login",
    "api",
    "webapp",
    "express",
    "materialize"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "cypress": "^14.5.3",
    "nodemon": "^3.0.1"
  }
}